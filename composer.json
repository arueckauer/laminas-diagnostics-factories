{
    "name": "arueckauer/laminas-diagnostics-factories",
    "description": "Configuration-based PSR-11 factories for laminas-diagnostics",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "arueckauer",
        "laminas-diagnostics",
        "factory",
        "health-check",
        "psr11"
    ],
    "authors": [
        {
            "name": "Andi RÃ¼ckauer",
            "email": "arueckauer@gmail.com",
            "homepage": "https://arueckauer.github.io/",
            "role": "Project Lead"
        }
    ],
    "homepage": "https://github.com/arueckauer/laminas-diagnostics-factories",
    "require": {
        "php": "~8.0.0 || ~8.1.0 || ~8.2.0",
        "laminas/laminas-diagnostics": "^1.24.0",
        "psr/container": "^2.0.2"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.39",
        "laminas/laminas-coding-standard": "^2.5",
        "phpunit/phpunit": "^10.4.2",
        "roave/security-advisories": "dev-latest"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "LaminasDiagnosticsFactories\\": "src/LaminasDiagnosticsFactories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasDiagnosticsFactoriesTest\\": "test/LaminasDiagnosticsFactories"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    }
}
